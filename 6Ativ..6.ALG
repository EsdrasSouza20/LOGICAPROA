Algoritmo "semnome"


Var
   Velocidade_KM, Distancia_KM, Ms: real
   Distancia: real

Inicio
   escreval ("informe a velocidade media do seu veiculo em Km/h")
leia(Velocidade_KM)

   Ms<-Velocidade_KM/3.6
     escreval ("esta velocidade Ms é ", Ms)
leia(Ms)

   Distancia_KM<- 434/Velocidade_KM
     escreval ("O tempo de SP para RJ é de", Distancia_KM, " horas")
leia(Distancia_KM)


Fimalgoritmo